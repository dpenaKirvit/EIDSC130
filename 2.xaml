                                            </dxga:ArcScaleMarker.Options>
                                            <dxga:ArcScaleMarker.Presentation>
                                                <dxga:MagicLightArcScaleMarkerPresentation Fill="Red"/>
                                            </dxga:ArcScaleMarker.Presentation>
                                        </dxga:ArcScaleMarker>
                                    </dxga:ArcScale.Markers>
                                    <dxga:ArcScale.LineOptions>
                                        <dxga:ScaleLineOptions Thickness="4"/>
                                    </dxga:ArcScale.LineOptions>
                                    <dxga:ArcScale.MajorTickmarkOptions>
                                        <dxga:MajorTickmarkOptions FactorLength="2" FactorThickness="2"/>
                                    </dxga:ArcScale.MajorTickmarkOptions>
                                    <dxga:ArcScale.LinePresentation>
                                        <dxga:DefaultArcScaleLinePresentation Fill="White"/>
                                    </dxga:ArcScale.LinePresentation>
                                    <dxga:ArcScale.Needles>
                                        <dxga:ArcScaleNeedle IsInteractive="True" Value="{Binding A1brud}" Visible="{Binding Vbrud}">
                                            <dxga:ArcScaleNeedle.Animation>
                                                <dxga:IndicatorAnimation/>
                                            </dxga:ArcScaleNeedle.Animation>
                                            <dxga:ArcScaleNeedle.Presentation>
                                                <dxga:DefaultArcScaleNeedlePresentation Fill="White"/>
                                            </dxga:ArcScaleNeedle.Presentation>
                                        </dxga:ArcScaleNeedle>
                                        <dxga:ArcScaleNeedle IsInteractive="True" Value="{Binding A2brud}" Visible="{Binding Vbrud}">
                                            <dxga:ArcScaleNeedle.Animation>
                                                <dxga:IndicatorAnimation/>
                                            </dxga:ArcScaleNeedle.Animation>
                                            <dxga:ArcScaleNeedle.Presentation>
                                                <dxga:DefaultArcScaleNeedlePresentation Fill="Cyan"/>
                                            </dxga:ArcScaleNeedle.Presentation>
                                        </dxga:ArcScaleNeedle>
                                    </dxga:ArcScale.Needles>
                                    <dxga:ArcScale.TickmarksPresentation>
                                        <dxga:DefaultTickmarksPresentation MinorTickBrush="White" MajorTickBrush="White"/>
                                    </dxga:ArcScale.TickmarksPresentation>
                                </dxga:ArcScale>
                            </dxga:CircularGaugeControl.Scales>
                        </dxga:CircularGaugeControl>
                    </Grid>
                </Grid>
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.8*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="0.6*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <!-- //////////////////////////////FUEL PRESS////////////////////////////// -->
                        <Border BorderBrush="Black" BorderThickness="2" HorizontalAlignment="Center" Height="24" Margin="0,0,0,0" VerticalAlignment="Center" Width="300">
                            <TextBlock TextAlignment="Center" TextWrapping="Wrap" Text="{Binding Fflp}" Foreground="Yellow"/>
                        </Border>
                    </Grid>
                    <!-- //////////////////////////////FLAPS////////////////////////////// -->
                    <Grid Grid.Column="2" HorizontalAlignment="Center" Height="200" Margin="0,0,0,0" VerticalAlignment="Center" Width="200">
                        <TextBlock TextAlignment="Center" HorizontalAlignment="Left" Margin="50,60,0,0" TextWrapping="Wrap" Text="FLAPS" Foreground="White" VerticalAlignment="Top" Height="30" Width="100" FontSize="20" Visibility="Visible"/>
                        <dxga:CircularGaugeControl >
                            <dxga:CircularGaugeControl.Scales>
                                <dxga:ArcScale EndAngle="100" HorizontalAlignment="Center" Height="240" MajorIntervalCount="5" MinorIntervalCount="2" ShowMinorTickmarks="True" ShowLabels="False" ShowLine="True" ShowMajorTickmarks="True" StartAngle="-100" VerticalAlignment="Center" Width="240">
                                    <dxga:ArcScale.Needles>
                                        <dxga:ArcScaleNeedle IsInteractive="True" Value="{Binding Fps}" Visible="{Binding Afps}">
                                            <dxga:ArcScaleNeedle.Animation>
                                                <dxga:IndicatorAnimation/>
                                            </dxga:ArcScaleNeedle.Animation>
                                            <dxga:ArcScaleNeedle.Options>
                                                <dxga:ArcScaleNeedleOptions EndOffset="45"/>
                                            </dxga:ArcScaleNeedle.Options>
                                            <dxga:ArcScaleNeedle.Presentation>
                                                <dxga:CleanWhiteArcScaleNeedlePresentation Fill="White"/>
                                            </dxga:ArcScaleNeedle.Presentation>
                                        </dxga:ArcScaleNeedle>
                                    </dxga:ArcScale.Needles>
                                    <dxga:ArcScale.MajorTickmarkOptions>
                                        <dxga:MajorTickmarkOptions FactorLength="2" FactorThickness="2"/>
                                    </dxga:ArcScale.MajorTickmarkOptions>
                                    <dxga:ArcScale.MinorTickmarkOptions>
                                        <dxga:MinorTickmarkOptions FactorThickness="2"/>
                                    </dxga:ArcScale.MinorTickmarkOptions>
                                    <dxga:ArcScale.TickmarksPresentation>
                                        <dxga:DefaultTickmarksPresentation MinorTickBrush="White" MajorTickBrush="White"/>
                                    </dxga:ArcScale.TickmarksPresentation>
                                    <dxga:ArcScale.LinePresentation>
                                        <dxga:DefaultArcScaleLinePresentation Fill="White"/>
                                    </dxga:ArcScale.LinePresentation>
                                    <dxga:ArcScale.LineOptions>
                                        <dxga:ScaleLineOptions Thickness="4"/>
                                    </dxga:ArcScale.LineOptions>
                                    <dxga:ArcScale.CustomLabels>
                                        <dxga:ScaleCustomLabel Content="UP" Foreground="White" FontSize="16" Margin="0,45,0,0"/>
                                        <dxga:ScaleCustomLabel Content="DN" Foreground="White" FontSize="16" Margin="0,-70,0,0" Value="100"/>
                                        <dxga:ScaleCustomLabel Content="20" Foreground="White" FontSize="16" Margin="-35,40,0,0" Value="20"/>
                                        <dxga:ScaleCustomLabel Content="40" Foreground="White" FontSize="16" Margin="-65,5,0,0" Value="40"/>
                                        <dxga:ScaleCustomLabel Content="60" Foreground="White" FontSize="16" Margin="-65,-30,0,0" Value="60"/>
                                        <dxga:ScaleCustomLabel Content="80" Foreground="White" FontSize="16" Margin="-40,-60,0,0" Value="80"/>
                                    </dxga:ArcScale.CustomLabels>
                                    <dxga:ArcScale.LabelOptions>
                                        <dxga:ArcScaleLabelOptions ShowLast="False" ShowFirst="False"/>
                                    </dxga:ArcScale.LabelOptions>
                                    <dxga:ArcScale.LabelPresentation>
                                        <dxga:DefaultScaleLabelPresentation />
                                    </dxga:ArcScale.LabelPresentation>
                                </dxga:ArcScale>
                            </dxga:CircularGaugeControl.Scales>
                        </dxga:CircularGaugeControl>
                        <TextBlock TextAlignment="Center" HorizontalAlignment="Left" Margin="0,0,0,0" TextWrapping="Wrap" Text="FLAPS" Foreground="Black" Background="Red" VerticalAlignment="Top" Height="200" Width="200" FontSize="70" Visibility="{Binding Mfps}"/>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
        <!-- //////////////////////////////GRID 3////////////////////////////// -->
    </Grid>
</Window>
